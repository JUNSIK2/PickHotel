<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="callSettersOnNulls" value="true" />
		<setting name="returnInstanceForEmptyRow" value="true" />
	</settings>
	<typeAliases>

		<!-- 회원 -->
		<typeAlias type="com.example.member.model.MemberVo" alias="MemberVo" />
		<typeAlias type="com.example.member.model.MemberStateVo" alias="MemberStateVo" />
		<typeAlias type="com.example.member.model.MemberAndMemberStateVo" alias="MemberAndMemberStateVo" />

		<!-- 회원 등업 -->
		<typeAlias type="com.example.grade.model.GradeUpVo" alias="GradeUpVo" />
		<typeAlias type="com.example.grade.model.MemberGradeVo" alias="MemberGradeVo" />

		<!-- 게시판 -->
		<typeAlias type="com.example.board.model.BoardVo" alias="BoardVo" />

		<!-- 게시글 -->
		<typeAlias type="com.example.board.model.AttachVo" alias="AttachVo" />
		<typeAlias type="com.example.board.model.CommentVo" alias="CommentVo" />
		<typeAlias type="com.example.board.model.PostVo" alias="PostVo" />
		<typeAlias type="com.example.board.model.ReviewVo" alias="ReviewVo" />
		<typeAlias type="com.example.board.model.RoomVo" alias="RoomVo" />

		<!-- 사이트 등급 -->
		<typeAlias type="com.example.grade.model.SiteGradeVo" alias="SiteGradeVo" />

		<!-- 알림 -->
		<typeAlias type="com.example.alarm.model.AlarmVo" alias="AlarmVo" />
	</typeAliases>
</configuration>